package rules
 
import  main.model.ReleaseModel
rule "Release open"
	when 
		Release: ReleaseModel(openPerc>0.5)
	then
		Release.setColorLabel("Red");
	end
rule "Release progress"
	when
		Release: ReleaseModel(progPerc>0.5)
	then
		Release.setColorLabel("Amber");
	end
rule "Release closed"
	when
		Release: ReleaseModel(closPerc>0.5)
	then
		Release.setColorLabel("Red");
	end
